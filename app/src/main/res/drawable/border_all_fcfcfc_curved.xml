<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/GBL5"/>
            <corners
                android:bottomRightRadius="5dp"
                android:bottomLeftRadius="5dp"/>
        </shape>
    </item>

    <item
        android:left="0dp"
        android:right="0dp"
        android:top="1dp"
        android:bottom="1dp">
        <shape android:shape="rectangle">
            <corners
                android:bottomRightRadius="5dp"
                android:bottomLeftRadius="5dp"/>
            <solid android:color="@color/white_fcfcfc"/>
        </shape>
    </item>
</layer-list>